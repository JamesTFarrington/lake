#!/bin/sh
#|-*- mode:lisp -*-|#
#|
exec ros -Q -- $0 "$@"
|#

(ql:quickload :clake :silent t)

(defun main (&optional target &rest argv)
  (declare (ignorable argv))
  (handler-case
      (if target
          (clake:clake :target target)
          (clake:clake))
    (error (e)
      (format t "~A~%" e)
      (ros:quit -1))))
