#!/bin/sh
#|-*- mode:lisp -*-|#
#|
exec ros -Q -- $0 "$@"
|#

(ql:quickload :clake :silent t)

(defun main (&rest argv)
  (declare (ignorable argv))
  (handler-case
      (if argv
          (loop for target in argv
             do (clake:clake :target target))
          (clake:clake))
    (error (e)
      (format t "~A~%" e)
      (ros:quit -1))))
